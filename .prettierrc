{
    "jsxSingleQuote": false,
    "printWidth": 160,
    "quoteProps": "consistent",
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 4,
    "plugins": ["@prettier/plugin-php"],
    "overrides": [
        {
            "files": ["*.php"],
            "options": {
                "parser": "php",
                "phpVersion": "8.3"
            }
        }
    ]
}
